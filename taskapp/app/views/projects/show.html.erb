<h1><%= @project.title  %></h1>

<ul>
<% @project.tasks.each do |task| %>
<li>
	<%= task.title %>
	<%= link_to "[回答削除]",project_task_path(task.project_id,task.id), method: :delete,data:{confirm:"削除しますか？"} %>
</li>
	<%end%>
<li>
	<%= form_for [@project, @project.tasks.build] do |f| %>
</li>
	<%= f.text_field :title %>
	<%= f.submit value:"回答作成" %>
	<%end%>


</ul>
